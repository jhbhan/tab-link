{
  "short_name": "React App",
  "name": "Create React App Sample",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "action": {
    "default_popup": "index.html"
  },
	"background": {
		"service_worker": "extension.js"
	},
  "chrome_url_overrides": {
      "newtab": "index.html"
  }
}
